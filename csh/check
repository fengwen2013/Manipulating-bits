set dir=/home/feng/WorkPlace/CSCI531HW/HW1/Manipulating-bits
#
# for the following commands, each correct answer gets 1 point
#
rm $dir/files/*.*
foreach f (1 2 3 4 5 6)
	echo "===> $dir/files/f$f"
	$dir/src/hw1 enc-base64 $dir/files/f$f > f$f.b64
	openssl enc -e -base64 -in $dir/files/f$f > s_e_f$f.b64
	diff f$f.b64 s_e_f$f.b64
end

